
#include <stdio.h>

int main()
{
    printf("RA2211042010059\n")
    int number1 , number2 , total;
    printf("Enter first number: ");
    scanf("%d", &number1);
    printf("Enter second number: ");
    scanf("%d", &number2);
    total = number1 + number2;
    printf("The SUM of %d and %d = %d", number1 , number2 , total);
    return 0;
    
   }
